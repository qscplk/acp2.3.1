    "rxjs": "^6.5.2",
    "sockjs-client": "^1.1.4",
    "uuid": "^3.3.2",
    "xterm": "^3.3.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@1stg/app-config": "^0.1.16",
    "@1stg/tslint-config": "^0.5.7",
    "@alauda/custom-webpack": "^0.4.2",
    "@angular-builders/custom-webpack": "^8.3.0",
    "@angular-devkit/build-angular": "^0.803.19",
    "@angular/cli": "^8.3.19",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@types/d3-array": "^2.0.0",
    "@types/d3-color": "^1.2.1",
    "@types/d3-drag": "^1.2.2",
    "@types/d3-format": "^1.3.1",
    "@types/d3-scale": "^2.0.1",
    "@types/d3-selection": "^1.3.4",
    "@types/d3-shape": "^1.2.3",
    "@types/d3-transition": "^1.1.3",
    "@types/d3-zoom": "^1.7.2",
    "@types/dagre": "^0.7.42",
    "@types/debug": "^4.1.2",
    "@types/file-saver": "^1.3.0",
    "@types/js-yaml": "^3.11.1",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^10.1.4",
    "@types/pluralize": "^0.0.29",
    "@types/ramda": "types/npm-ramda#dist-simple",
    "@types/uuid": "^3.4.5",
    "serve-handler": "^6.1.2",
    "ts-essentials": "^2.0.11",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1"
  },
  "resolutions": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "eslint-plugin-prettier": "^3.1.1",
    "typescript": "~3.5.3"
  },
  "browserslist": [
    "extends @1stg/browserslist-config/modern"
  ],
  "commitlint": {
    "extends": [
      "@1stg"
    ]
  },
  "eslintConfig": {
    "extends": "@1stg/eslint-config/loose"
  },
  "eslintIgnore": [
    "dist",
    "!/.*.js"
  ],
  "prettier": "@1stg/prettier-config/semi/angular",
  "remarkConfig": {
    "plugins": [
      "@1stg/remark-config"
    ]
  },
  "stylelint": {
    "extends": "@1stg/stylelint-config/scss/loose",
    "rules": {
      "selector-type-no-unknown": [
        true,
        {
          "ignoreTypes": [
            "/^(acl|alo|aui|ng)-/"
          ]
        }
      ]
    }
  }
}
